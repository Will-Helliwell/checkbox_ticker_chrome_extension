{
    "manifest_version": 3,
    "name": "Tick all checkboxes",
    "description": "Extension that ticks all checkboxes in the current tab",
    "version": "1.0.0",
    "icons": {
      "16": "images/green_tick_16.png",
        "48": "images/green_tick_48.png",
        "128": "images/green_tick_128.png"
    },
    "action": {
      "default_popup": "pop_up.html",
      "default_title": "Tick all checkboxes"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ]
  }